            fx:controller="com.example.gerenteapp.LangileakKudeatuController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="langileTable" layoutX="20.0" layoutY="20.0" prefHeight="360.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="idColumn" text="id" prefWidth="100.0"/>
            <TableColumn fx:id="izenaColumn" text="Izena" prefWidth="200.0"/>
            <TableColumn fx:id="abizenaColumn" text="Abizena" prefWidth="200.0"/>
        </columns>
    </TableView>

</AnchorPane>

